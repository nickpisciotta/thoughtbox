<!DOCTYPE html>
<html>
  <head>
    <title>Thoughtbox</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>

  <nav class='navbar'>
    <% if current_user %>
      <%= link_to "Log Out", logout_path, method: :delete %>
    <% else %>
      <%= link_to "Log In", login_path %> or <%= link_to "Sign Up", new_user_path %>
    <% end %>
  </nav>
    <%= yield %>
  </body>
</html>
